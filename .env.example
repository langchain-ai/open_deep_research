# --- LLM runtime (Open Deep Research) ---
LLM_MODE=api
LLM_PROVIDER=openrouter
LLM_API_KEY=
LLM_BASE_URL=https://openrouter.ai/api/v1
LLM_MODEL=mistralai/mistral-small-3.1-24b-instruct:free
LLM_TEMPERATURE=0
LLM_TIMEOUT_SECONDS=60
LLM_MAX_TOKENS=
LLM_SEED=

# --- Optional tracing (LangSmith) ---
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=
SEC_TELEMETRY_STRIP_UNTRUSTED=true

# --- LLAMATOR models (attack/judge) ---
LLAMATOR_ATTACK_API_KEY=
LLAMATOR_ATTACK_BASE_URL=https://openrouter.ai/api/v1
LLAMATOR_ATTACK_MODEL=google/gemma-3-12b-it:free

LLAMATOR_JUDGE_API_KEY=
LLAMATOR_JUDGE_BASE_URL=
LLAMATOR_JUDGE_MODEL=

# --- Open Deep Research endpoint (dev-server) ---
ODR_LANGGRAPH_URL=http://127.0.0.1:2024
ODR_GRAPH_NAME=Deep Researcher

# --- CI safety ---
LIVE_API=0
