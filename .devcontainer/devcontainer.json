{
    "name": "Open Deep Research Dev Container",
    "image": "mcr.microsoft.com/devcontainers/python:3.11",
    "features": {
        "ghcr.io/devcontainers/features/git-lfs:1": {}
    },
    "postCreateCommand": "curl -LsSf https://astral.sh/uv/install.sh | sh && uv sync",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        }
    }
}